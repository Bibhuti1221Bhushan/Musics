configuration {
	display-emoji: "ï›´";
	show-icons: false;
	me-select-entry: "";
	me-accept-entry: "MousePrimary";
}

* {
	font: "Noto Sans 11";
	text-color: #eff0eb;
	background-color: #1917244D;
	border-color: #00000000;
	width: 684px;
	height: 455px;
	location: 0;
	border-radius: 8px; 
}

inputbar {
	border: 0 0 1px 0;
	children: [prompt,entry];
	border-radius: 5px;
}

prompt {
	font: "Noto Sans 18";
	vertical-align: 0.5;
	padding: 0px 9px 0px 9px;
	border: 0 1px 0 0;
	border-radius: 5px;
}

entry {
	padding: 12px;
	border-radius: 5px;
}

textbox {
	background-color: #1917244D;
	border-color: #1917244D;
	padding: 13px 13px;
	border-radius: 8px;
}

listview {
	enabled: true;
	columns:                     5;
	 fixed-height:                true;
    fixed-columns:               true;
	cycle: false;
	scrollbar: false;
	columns: 9;
	cycle: true;
	dynamic: false;
	layout: vertical;
	padding: 8px 10px 10px 10px;
	border-radius: 8px;
}

element {
	border-radius: 3px;
	padding: 14px 0px 14px 0px;
}

element-text {
	font: "Noto Color Emoji 27";
	expand: false;
	text-color: #333333;
	horizontal-align: 1;
	vertical-align: 0.5;
	margin: 0px 0px 0px 13px;
	width: 1px;
	padding: 1px 0px 16px;
	border-radius: 8px;
}

element selected {
	background-color: #191724;
}